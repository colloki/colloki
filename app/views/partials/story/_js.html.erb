$(document).ready(function(e) {
  this.likeView = new LikeView({
    el: $(".like-btn", $(".story")),
    story: <%= story.to_json(:include => :votes).html_safe %>,
    current_user: <%= current_user.to_json.html_safe %>
  });

  var comments = new window.CommentsListView({
    el: $("#comments<%= story.id %>"),
    data: <%= story.comments.to_json(:methods => :user).html_safe %>,
    story_id: <%= story.id %>,
    user_id: <%= current_user ? current_user.id : -1 %>
  });

});
