<div id="middle">
  <h2>Fetch a new story</h2>
  <%= form_tag("/story_fetcher/parse") do %>
  <%= label_tag "Story url:" %>
  <%= text_field_tag :url, "", :size => 100 %>
  <br><br>
  <%= submit_tag("Fetch!") %>
  <% end %>
  <br><br>
  <div style="color: #555; font-size: 11px">Currently supported sites: collegiatetimes.com, roanoke.com, vtnews.vt.edu</div>
</div>